[GLOBAL] 
//全局设置 
 
UseEffect=true 
//总特效开关 
//打开或关闭全局特效开关只会对ENB的视觉效果起作用，不会影响到ENB性能方面的功能（比如enblocal.ini里的选项）。几乎所有的版本都可以用SHIFT+F12来开启或关闭全局特效，这个热键可以在enblocal.ini里的[INPUT]里设置。 


 
[EFFECT] 
//特效开关 
 
UseOriginalPostProcessing=false  
//是否使用原版后期处理 
//当开启的时候，ENB会采用原版游戏的颜色处理方式。这个可以在想要使用ENB的某些特效（比如DOF）来查看原版游戏的表现时开启。 
 
UseOriginalObjectsProcessing=false  
//是否使用原版对象处理 
//和上一个选项相似，只不过仅对游戏中的某些物品和模型有效。 

EnableBloom=true 
//漫反射 
//开启ENB的漫反射特效，它会在物体的表面添加漫反射光效。物体的亮度越高，漫反射效果越明显。其滤镜文件为enbbloom.fx。 

EnableLens=true 
//镜头特效 
//模拟光线进入镜头时在镜头表面产生的效果，其滤镜档为enblens.fx。 

EnableAdaptation=true 
//人眼自我调整 
//顾名思义，会让游戏跟人眼一样处理明亮和昏暗的区域，亮度会逐渐地调节变化，取决于镜头视角所在的位置，在阴影处对光线敏感，在明亮处对光线麻木。简而言之在亮处画面会变暗，暗处会变亮。 

EnableDepthOfField=true 
//景深 
//开启景深特效，景深是指在游戏镜头能够取得清晰图像的成像时所测定的被摄物体前后距离范围。在聚焦完成后，在焦点前后的范围内都能形成清晰的像，这一前一后的距离范围，便叫做景深。在景深之内的影像比较清楚，在这个范围之前或是之后的影像则比较模糊。其滤镜档为后的范围内都能形成清晰的像，这一前一后的距离范围，便叫做景深。在景深之内的影像比较清楚，在这个范围之前或是之后的影像则比较模糊。其滤镜档为后的范围内都能形成清晰的像，这一前一后的距离范围，便叫做景深。在景深之内的影像比较清楚，在这个范围之前或是之后的影像则比较模糊。其滤镜档为后的范围内都能形成清晰的像，这一前一后的距离范围，便叫做景深。在景深之内的影像比较清楚，在这个范围之前或是之后的影像则比较模糊。其滤镜文件为enbeffectprepass.fx。

EnableAmbientOcclusion=true 
//屏幕空间环境光遮蔽和屏幕空间间接光照 
//开启屏幕空间环境光屏蔽（Screen Space Ambient Occlusion，SSAO），用于在图形中实时实现近似环境光屏蔽效果的渲染技术。该算法作为像素着色器，通过分析场景中纹理的深度值缓冲来实现，可以近似地表现出物体在环境光下产生的阴影。 
//开启屏幕空间间接光照（Screen Space Indirect Illumination ，SSIL），可以模拟场景中由于间接光照接收量不同而呈现的明暗变化，间接光照不仅可以减轻过度阴影的效果，更可以用多样的颜色（如染色现象）进一步提高真实性。 

EnableDetailedShadow=true 
//细致阴影 
//细致阴影和SSAO不同，比SSAO在帧数优化上要高非常多，它主要修改了阴影的投射地点，使画面看起来更和谐自然，这个特效也修正了原版游戏的阴影条状bug。 

EnableSunRays=true 
//上帝光 
//太阳射线（上帝光）在太阳四周加入散射的可见光射线，使得看向太阳时的画面非常饱满，许多著名的3D大作都使用此特效。 

EnableSunGlare=true 
//太阳光晕 
//模拟太阳光射入镜头的效果，其滤镜档为enbsunsprite.fx，光晕的贴图文件为enbsunsprite.tga。 

EnableSkyLighting=true 
//真实阴影普照 
//调节阴影外的所有区域的亮度，并使阴影从里到外的亮度逐渐变亮，给肉眼一个阴影深度的错觉。 

EnableImageBasedLighting=true 
//图像照明 
//开启图像照明（Image-based lighting，IBL），一种计算机虚拟场景渲染技术，通过把一个图像使用纹理映像技术放置到一个虚拟的简单几何表面上，该表面包裹被照明的主要三维场景（包括角色、环境和道具等），使用全局光照技术，把该虚拟表面作为照亮三维场景的光源，从而使渲染出的画面更为真实，就像它真的处于图像所描述的环境一样。许多特效非常出色的电影中都使用了图像照明技术，因为能够模拟出非常逼真的照明效果，使计算机生成图像能够无缝地结合到实拍的画面中，该技术正越来越多地被加入到特效制作流程中。 

EnableReflection=true 
//实时反射 
//在物体表面添加动态反射的特效，它的算法和处理方式类似于SSAO，同时对[ENVIRONMENT]下的反光数值SpecularAmount有直接影响。 

EnableSoftParticles=true  
//软粒子 
//柔化某些效果四周的一圈粒子，比如火焰。 

EnableParticleLights=true 
//粒子光 
//任何人造光源（火源 烛光 盏灯等）四周的一圈星形光晕特效，可以在[LightSprite]下设置。 

EnableSubSurfaceScattering=true 
//皮下光线散射 
//使光渗透进物体表面，在物体内从不规则的角度多次反射开来，从而达到在物体表面呈现出清透的效果，在ENB内主要用于调节皮肤/皮毛上。 

EnableWater=true  
//水面特效 
//调节从水外往水面看的波纹、反射和清透度等多种特效。 

EnableUnderwater=true 
//水下特效 
//调节从水下往水面看的波纹、反射和清透度等多种特效。 

EnableCloudShadows=true  
//云层阴影 
//让云层遮蔽部分太阳光照从而造成云层投射阴影的错觉 。 

EnableVolumetricRays=true  
//体积射线 
//将上方的太阳射线特效进一步处理成大面积的太阳射线散射。 

EnableProceduralSun=true 
//太阳处理 
//不同于上帝光，太阳处理可以对太阳的光线和模型进行简单的修改。 

EnableMist=true  
//雾气特效 
//添加了可调整的雾气环绕效果。 



[COLORCORRECTION] 
//颜色校正 

UsePaletteTexture=true 
//是否读取色板档 
//开启将允许一张256×256的渐变色位图enbpalette.bmp来校正游戏内场景的颜色，这将完全覆盖整个游戏场景的色调。 

Brightness=1.0 
GammaCurve=1.0 
//色彩亮度修正与伽马值修正 
//这些参数将会调整游戏内颜色的亮度与伽马曲线，可以配合游戏内的亮度调整，使游戏画面达到平衡。如果不是有特殊用途，不建议调整这两项。 

UseProceduralCorrection=true 
//使用程序修正 
//开启此项之后才能够对shader进行颜色和亮度等等的调整，也就说enbeffect.fx中的IDR参数才能被启动。



[WEATHER] 
//天气系统 

EnableMultipleWeathers=true  
//开启自定义天气 
//开启自定义天气后，针对每一种不同的天气的Form ID，将会对应一个独有的ini檔，不过这个ini文件的选项有限，部分参数还是由enbseries.ini里全局控制。需要enbhelper.dll支持：http:www.dev-c.com/skyrim/ENBhelper/ 



[TIMEOFDAY] 
//时段特效 

Enable=true  
//开启 
//开启之后取代原版的日出日落，以及白天黑夜的时间与时长。注意每个时段都不是绝对的，比如在两个时段之间，会按时间差的百分比混合两个时段的设置。 

DawnDuration=2.0 
//日出持续时间 
//数值2.0即表示黎明时间为2小时。 

SunriseTime=7.5   
//日出时刻 
//数值7.5即表明早上7点30分日出。 

DayTime=13.0   
//白天时刻 
//数值表示正午时刻，即白天最明亮的时刻为中午13点。 

SunsetTime=18.5  
//黄昏时刻 
//数值表示18点30分开始日落。 

DuskDuration=2.0  
//黄昏持续时间 
//数值2.0即表示黄昏时间持续2小时。 

NightTime=1.0   
//夜晚时刻 
//数值1.0表示夜晚最黑暗的时刻是凌晨1点。 



[NIGHTDAY] 
//昼夜过渡 

DetectorOldVersion=false  
//旧版的检测方式 
//Boris Vorontsov的解释：该项是在很老版本的ENB中所使用的昼/夜检测的代码，它主要基于天空半球的亮度（准确地说是地平线的部分）。当设置为false时，一天的昼/夜时间计算直接从游戏时间获取（其实是一样的） 。 

DetectorDefaultDay=false   
//游戏预设的白天检测方式 
//开启的话，会忽略下面的设置，采用游戏预设的检测方式 

DetectorLevelDay=0.65  
//白天侦测等级 
//越大会使得侦测更加依赖于白天。 

DetectorLevelNight=0.25  
//夜晚侦测等级 
//越大会使得侦测更加依赖于夜晚。 

DetectorLevelCurve=1.0 
//侦测曲线 
//越大会使得夜晚的侦测变得敏感，越小会使得白天的侦测变得敏感。 



[ADAPTATION] 
//人眼自我调整 

ForceMinMaxValues=false  
//强制自我调整的最大最小亮度 
//开启该选项会使得最大与最小亮度被固定 

AdaptationSensitivity=0.2  
//自我调整的敏感程度 
//敏感程度的数值是自我调整区分场景明暗的阈值，越大的话自我调整的效果越明显，反之则反 

AdaptationTime=0.4  
//自我调整的时间   
//自我调整的由明变暗或由暗变明所持续的时间 

AdaptationMin=0.1  
AdaptationMax=100.0  
//允许自我调整的最小与最大亮度 
//在开启了第一项的前提下，最小亮度越低，画面在暗的场景就越亮，因为自我调整会在亮度高于AdaptationMin时启动；在暗的场景中看亮的地方，AdaptationMax越高，亮的地方就越亮。在明亮的场景下的情况可以此类推。 



[BLOOM] 
//漫反射 

AmountSunrise=0.25  
AmountDay=0.25 
AmountSunset=0.25 
AmountNight=0.25 
AmountInteriorDay=0.25 
AmountInteriorNight=0.25 
//漫反射强度 
//数值越高，物体漫反射越强，即泛光越强，太高了会产生朦胧感。 

BlueShiftAmountSunrise=1.0  
BlueShiftAmountDay=1.0 
BlueShiftAmountSunset=1.0 
BlueShiftAmountNight=1.0 
BlueShiftAmountInteriorDay=1.0 
BlueShiftAmountInteriorNight=1.0 
//漫反射蓝移 
//影响漫反射中的蓝色信道，这个选项和漫反射强度的设置有关系，如果漫反射强度很低，则该项影响不明显。此数值在大于1的时候会增强漫反射中的蓝色，在小于1的时候则会削减漫反射中的蓝色。 



[LENS] 
//镜头特效 

IgnoreWeatherSystem=false 
//忽略天气系统 
//开启使自定义天气下对应该标签内的ini选项被忽略，即enbseries.ini内这些选项设定会变成所有天气的全局设定。 

ReflectionIntensitySunrise=1.0   
ReflectionIntensityDay=1.0 
ReflectionIntensitySunset=1.0 
ReflectionIntensityNight=1.0 
ReflectionIntensityInteriorDay=1.0 
ReflectionIntensityInteriorNight=1.0 
//辉光亮度 
//数值越大， 火光或者魔法效果在镜头上产生的辉光越亮。 

ReflectionPowerSunrise=2.0   
ReflectionPowerDay=2.0 
ReflectionPowerSunset=2.0 
ReflectionPowerNight=2.0 
ReflectionPowerInteriorDay=2.0 
ReflectionPowerInteriorNight=2.0 
//辉光权重 
//数值越大，火光或者魔法效果在镜头上产生的辉光敏感程度越低，同时使得辉光更加分散。 

DirtIntensitySunrise=1.0   
DirtIntensityDay=1.0 
DirtIntensitySunset=1.0 
DirtIntensityNight=1.0 
DirtIntensityInteriorDay=1.0 
DirtIntensityInteriorNight=1.0 
//光斑亮度 
//数值越大，火光或者魔法效果在镜头上产生的光斑越亮。 

DirtPowerSunrise=2.0 
DirtPowerDay=2.0 
DirtPowerSunset=2.0 
DirtPowerNight=2.0 
DirtPowerInteriorDay=2.0 
DirtPowerInteriorNight=2.0 
//光斑权重 
//数值越大，火光或者魔法效果在镜头上产生的光斑敏感程度越低。 



[SSAO_SSIL] 
//屏幕空间环境光遮蔽和屏幕空间间接光照 

EnableSupersampling=false
//开启超级取样 
//四倍的超级取样（2*2），能够获得更加平滑的效果，但对帧数有一定影响。开启之后，Denoiser以及FilterQuality将对画面不再有影响。 

UseIndirectLighting=true
//使用间接光照 
//开启该选项会对帧数有一定的影响，但会偏置AO效果，以此来对抗AOamount对阴影的加深效果，会使得光照效果更加真实，这项决定了SSAO_SSIL的快慢模式。 

UseComplexIndirectLighting=false
//使用复杂的间接照明 
//开启该选项会对帧数有一定的影响，这将允许更详细的间接照明被添加至场景中，这使得IL得到更好的表现。 

UseComplexAmbientOcclusion=false
//使用复杂的环境光吸收 
//开启该选项会对帧数有一定的影响，这将允许更详细的环境遮蔽被添加至场景中，这使得AO得到更好的表现。 

UseAmbientIndirectLighting=false
//使用全局环境间接光照 
//开启该选项会对帧数有一定的影响，这个设置将使间接照明得到延伸，这样间接照明场景中可以在场景中被添加到新的地方。 

SamplingQuality=2
//采样品质 
//在SamplingQuality=2也就是低质量下，由于质量过低会造成明显的分辨率损失，会极大影响IL及AO的效果，可能会产生网格状阴影。 

SamplingPrecision=2
//采样精确度 
//该项决定了阴影轮廓的噪声采样精度，在SSAO_SSIL里对性能的影响较其他几项略小。 

SamplingRange=0.3
//采样半径 
//Boris Vorontsov的原话：该项为场景里的每个像素周围读取纹理的相对半径，较低的值可以为距离较近的物体带来更好的阴影细节，但会损失部分阴影的几何特性；较高的值会由于缓存丢失而损失性能，因此这个值不建议设置太高。 

FadeFogRange=1.0
//SSAO雾渐隐半径 
//SSAO相对雾气距离的渐隐距离，如果在有雾的场景，增加这个值会逐渐移除SSAO的效果，减少这个值则会保留更多的SSAO渲染。 

SizeScale=1.0    
//尺寸规模 
//SSAO纹理的大小，越高效果越好，性能影响呈与屏幕分辨率有关。设置高于1.0是不推荐的，因为这意味着会SSAO纹理尺寸比屏幕分辨率大，徒增负荷。该项对性能的影响是非线性的，SizeScale=1.0会比SizeScale=0.5慢四倍。

SourceTexturesScale=1.0
//源纹理规模 
//SSAO读取的纹理规模，越高效果越好，同样地不推荐设置高于1.0，原因同采样半径，由于读取纹理的缓存丢失而损失性能。 

EnableComplexFilter=true
//使用复杂滤镜 
//Boris Vorontsov的解释：EnableComplexFilter可以有效地减少AO噪音，同时不会过多模糊AO贴图。当启用时，过滤器品质（FilterQuality）可以设置为较低的等级来得到同等质量。推荐开启，除非有人把其他环境闭塞选项设置到最高。我在天际ENB主题的版面中对这个滤镜做过对比截图。 

FilterQuality=2
//过滤品质 
//对SSAO的过滤计算越复杂度，对性能有一定的影响。 

FilterType=2 
//滤镜类型 
//在大多数情况下，类型0被公认为效果最好，类型4是0.145核心之前的滤镜类型。类型最好自己全部尝试一遍，来选择适合自己的。 

AOAmount=2.0 
AOAmountInterior=2.0 
//环境光吸收迭加度 
//在物体阴影周围的黑暗的迭加度，值越大，黑暗范围越扩展。 

ILAmount=6.0
ILAmountInterior=6.0 
//环境光折射迭加度 
//在AO阴影中增加的间接光照的量，值越大，间接光照越扩展。 

AOIntensity=2.0 
AOIntensityInterior=1.0 
//环境光吸收强度 
//AO的强度，值越大，AO阴影越黑暗。 

AOType=1 
//环境光吸收类型 
//Boris Vorontsov的原话：AOType=2是新的SSAO类型，噪点与AOType=1相似，比起AOType=0能提供更多细节。 
//类型最好自己全部尝试一遍，以此来选择适合自己的。 

ILType=0 
//环境光折射类型 
//类型最好自己全部尝试一遍，来选择适合自己的。 

AOMixingType=0 
AOMixingTypeInterior=2 
//环境光吸收混合类型 
//0.122核心加入的选项，类型最好自己全部尝试一遍，以此来选择适合自己的。 

EnableDenoiser=true 
//开启降噪 
//Boris Vorontsov的解释：另一种类型的噪声消除代码，它将在法线贴图为环境光遮挡计算之前，为法线贴图做抗锯齿，所以它能基本上消除锯齿边缘，但会在AO附近产生的噪音（副作用）。  



[SKYLIGHTING] 
//真实阴影普照 

IgnoreWeatherSystem=false 
//忽略天气系统 
//开启使自定义天气下对应该标签内的ini选项被忽略，即enbseries.ini内这些选项设定会变成所有天气的全局设定。 

Quality=1 
//品质 
//真实阴影普照阴影的质量，对性能有一定影响。 

FilterQuality=2 
//滤镜品质 
//滤镜用以对阴影进行过滤计算，对性能有一定影响。 

AmbientMinLevelSunrise=0.1 
AmbientMinLevelDay=0.1 
AmbientMinLevelSunset=0.1 
AmbientMinLevelNight=0.1 
AmbientMinLevelInteriorDay=0.1 
AmbientMinLevelInteriorNight=0.1 
//最小环境光照 
//数值越小阴影越重。以及阴影内的细节表现越不明显。如果低于0.8的话，在阴影下转动视角，会有明显的整体亮度的闪烁错误。 



[ENVIRONMENT] 
//环境 

IgnoreWeatherSystem=false 
//忽略天气系统 
//开启使自定义天气下对应该标签内的ini选项被忽略，即enbseries.ini内这些选项设定会变成所有天气的全局设定。 

DirectLightingIntensitySunrise=1.0 
DirectLightingIntensityDay=1.0 
DirectLightingIntensitySunset=1.0 
DirectLightingIntensityNight=1.0 
DirectLightingIntensityInteriorDay=1.0 
DirectLightingIntensityInteriorNight=1.0 
//直接光照强度 
//全局光线所照处的亮度，即曝光度，值越高，对比度越高。即亮的越亮，暗的越暗。 

DirectLightingCurveSunrise=1.0 
DirectLightingCurveDay=1.0 
DirectLightingCurveSunset=1.0 
DirectLightingCurveNight=1.0 
DirectLightingCurveInteriorDay=1.0 
DirectLightingCurveInteriorNight=1.0 
//直接光照色调曲线 
//光线所照处的色调，数值越高，暖色调的光照就越偏暖、偏红；冷色调时，数值越高，越偏蓝，尤其在晚上，阴影也相对变重。 

DirectLightingDesaturationSunrise=0.0 
DirectLightingDesaturationDay=0.0 
DirectLightingDesaturationSunset=0.0 
DirectLightingDesaturationNight=0.0 
DirectLightingDesaturationInteriorDay=0.0 
DirectLightingDesaturationInteriorNight=0.0 
//直接光照去饱和度 
//越高色调就越被抵消，直接光照的色彩就越偏向黑白，设成0代表保持原来的光照色彩。 

SpecularAmountMultiplierSunrise=1.0 
SpecularAmountMultiplierDay=1.0 
SpecularAmountMultiplierSunset=1.0 
SpecularAmountMultiplierNight=1.0 
SpecularAmountMultiplierInteriorDay=1.0 
SpecularAmountMultiplierInteriorNight=1.0 
