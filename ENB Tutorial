IgnoreWeatherSystem=false 
//忽略天气系统 
//开启使自定义天气下对应该卷标内的ini选项被忽略，即enbseries.ini内这些选项设定会变成所有天气的全局设定。 

EnableShadows=true 
//开启阴影 
//开启体积雾在光照下产生的阴影。 

IntensitySunrise=1.0 
IntensityDay=1.0 
IntensitySunset=1.0 
IntensityNight=1.0 
IntensityInteriorDay=1.0 
IntensityInteriorNight=1.0 
//体积雾亮度 
//调整山体与沼泽云雾的亮度。 

CurveSunrise=1.0 
CurveDay=1.0 
CurveSunset=1.0 
CurveNight=1.0 
CurveInteriorDay=1.0 
CurveInteriorNight=1.0 
//体积雾曲线 
//数值越高，山体与沼泽云雾色调越暖。 

LightingInfluenceSunrise=1.0 
LightingInfluenceDay=1.0 
LightingInfluenceSunset=1.0 
LightingInfluenceNight=1.0 
LightingInfluenceInteriorDay=1.0 
LightingInfluenceInteriorNight=1.0 
//体积雾照明影响 
//该项决定了体积雾会受到多少周围光照的影响，数值越高，雾气会反映出更多当前场景的光线与颜色。 

OpacitySunrise=1.0 
OpacityDay=1.0 
OpacitySunset=1.0 
OpacityNight=1.0 
OpacityInteriorDay=1.0 
OpacityInteriorNight=1.0 
//体积雾不透明度 
//该项决定了体积雾的ALPHA层的不透明度，设为0会使得山体与沼泽雾气消失。 



[FIRE] 
//火焰 

IgnoreWeatherSystem=false 
//忽略天气系统 
//开启使自定义天气下对应该卷标内的ini选项被忽略，即enbseries.ini内这些选项设定会变成所有天气的全局设定。 

IntensitySunrise=4.0 
IntensityDay=4.0 
IntensitySunset=4.0 
IntensityNight=4.0 
IntensityInteriorDay=4.0 
IntensityInteriorNight=4.0 
//火焰亮度 
//值越高火焰越偏白。此项数值只影响火焰的效果，不会影响环境亮度。 

CurveSunrise=1.0 
CurveDay=1.0 
CurveSunset=1.0 
CurveNight=1.0 
CurveInteriorDay=1.0 
CurveInteriorNight=1.0 
//火焰曲线 
//数值越高，火焰颜色越偏红。同样也只改变火焰色调，不会影响环境色调。 

AdditiveBlending=true 
//火焰交融效果 
//开启该项会使得火焰贴图的亮度迭加，在Intensity设置较高的情况下，会使火焰迭加的部分变成炫目的白色。 



[PARTICLE] 
//粒子光 

IgnoreWeatherSystem=false 
//忽略天气系统 
//开启使自定义天气下对应该卷标内的ini选项被忽略，即enbseries.ini内这些选项设定会变成所有天气的全局设定。 

IntensitySunrise=1.0 
IntensityDay=1.0 
IntensitySunset=1.0 
IntensityNight=1.0 
IntensityInteriorDay=1.0 
IntensityInteriorNight=1.0 
//粒子光亮度 
//例如瀑布水花的亮度、火焰上方的雾气、温泉的烟雾等。 

LightingInfluenceSunrise=1.0 
LightingInfluenceDay=1.0 
LightingInfluenceSunset=1.0 
LightingInfluenceNight=1.0 
LightingInfluenceInteriorDay=1.0 
LightingInfluenceInteriorNight=1.0 
//粒子光照影响 
//该项决定了粒子会受到多少周围光照的影响，数值越高，粒子会反映出更多当前场景的光线与颜色。 



[DEPTHOFFIELD] 
//景深 

IgnoreWeatherSystem=false 
//忽略天气系统 
//开启使自定义天气下对应该卷标内的ini选项被忽略，即enbseries.ini内这些选项设定会变成所有天气的全局设定。 

Quality=0 
//品质 
//对于新版本的景深来说，该项影响几乎可以忽略不计。 

FadeTime=1.0 
//DOF对焦时间 
//景深对焦点响应的时间，数值越小反应越快。 



[SHADOW] 
//阴影 

IgnoreWeatherSystem=false 
//忽略天气系统 
//开启使自定义天气下对应该卷标内的ini选项被忽略，即enbseries.ini内这些选项设定会变成所有天气的全局设定。 

ShadowCastersFix=false 
//阴影投影修复 
//在游戏中，某些地方可能不会投下阴影，开启该项可以使得阴影无处不在。 

UseBilateralShadowFilter=true 
//使用双向阴影过滤  
//保留阴影硬边，降低噪点，同时平滑阴影的边缘。注意这并不是模糊平滑。 

ShadowQualityFix=true 
//阴影质量修复 
//主要通过增加阴影的规模和质量略，模糊阴影的边缘，需要正确的skyrimprefs.ini设置，才能起到好的效果。 

DetailedShadowQuality=1 
//阴影细节质量 
//质量越高阴影越丰富，在越小的物体上会产生更细致的阴影。 

ShadowFilterQuality=2 
//阴影过滤质量 
//质量越高，阴影的噪点越少，尤其在ShadowBlurRange较高是越为明显。 

ShadowBlurRange=2.0 
ShadowBlurRangeInterior=2.0 
//阴影模糊半径 
//数值越大，阴影边缘越模糊。 



[RAYS] 
//上帝光 

IgnoreWeatherSystem=false 
//忽略天气系统 
//开启使自定义天气下对应该标签内的ini选项被忽略，即enbseries.ini内这些选项设定会变成所有天气的全局设定。 

SunRaysMultiplierSunrise=0.4 
SunRaysMultiplierDay=0.4 
SunRaysMultiplierSunset=0.4 
SunRaysMultiplierNight=0.4 
SunRaysMultiplierInteriorDay=0.4 
SunRaysMultiplierInteriorNight=0.4 
//上帝光系数 
//数值越高，上帝光越强，该项同时受到[SKY]里太阳设置Sun Intensity/Desaturation和SunColorFilter的影响。 



[REFLECTION] 
//实时反射 

IgnoreWeatherSystem=false 
//忽略天气系统 
//开启使自定义天气下对应该卷标内的ini选项被忽略，即enbseries.ini内这些选项设定会变成所有天气的全局设定。 

Quality=1 
//品质 
//质量越高，反射效果越好，对性能有一定的影响。 

FilterQuality=2 
//过滤品质 
//过滤质量越高，反射表面的噪点越少，对性能有一定的影响。 

FilterBluriness=0.0 
//过滤模糊值 
//数值越高，噪点会越不明显，但反射会变得越模糊。 

SizeScale=1.0 
//尺寸规模 
//反射纹理的大小，越高效果越好，性能影响呈与屏幕分辨率有关。设置高于1.0是不推荐的，因为这意味着会纹理尺寸比屏幕分辨率大，徒增负荷。该项对性能的影响是非线性的，SizeScale=1.0会比SizeScale=0.5慢四倍。 

SourceTexturesScale=1.0 
//源纹理规模 
//反射读取的纹理规模，越高效果越好，同样地不推荐设置高于1.0，原因是由于读取纹理的缓存丢失而损失较多性能。 

Amount=1.0 
//反射量 
//数值越高，场景中的反射越强。 

Power=1.0 
//反射权重 
//数值越高，产生反射的敏感度越低。 

GlosinessMin=0.0 
GlosinessMax=1.0 
//最小与最大光泽度 
//Boris Vorontsov的原话：GlosinessMin/Max会根据反射向量（一种软阴影参数，但是是用来模拟反射的粗糙程度的）的距离来决定反射量的多少。简单地来说，就是用来决定反射量的多少。

EnableDenoiser=true 
//开启降噪 
//开启一个可以降低噪点的降噪器 

DenoiserType=4 
//降噪器类型 
//Boris Vorontsov的原话：每种类型都有其不同之处，有些会在SizeScale较大的时候，表现较好，有些则不然。最好自行试验。 

EnableSupersampling=false 
//开启超级取样 
//类似于SSAO的4倍超级采样，会对性能有一定影响。 

ExteriorEnable=true 
InteriorEnable=true 
//开启室内或室外的实时反射 
//由此可以分离室内外的实时反射，比如想要在室外开启室内关闭的这种情况。 



[IMAGEBASEDLIGHTING] 
//图像照明 

IgnoreWeatherSystem=false 
//忽略天气系统 
//开启使自定义天气下对应该标签内的ini选项被忽略，即enbseries.ini内这些选项设定会变成所有天气的全局设定。 

AdditiveAmountSunrise=0.05 
AdditiveAmountDay=0.05 
AdditiveAmountSunset=0.05 
AdditiveAmountNight=0.05 
AdditiveAmountInteriorDay=0.05 
AdditiveAmountInteriorNight=0.05 
//添加量 
//值越高，图像照明就越明显，相当于把颜色冲淡一点，全局偏白色一点，偏向真实。 

MultiplicativeAmountSunrise=0.0 
MultiplicativeAmountDay=0.0 
MultiplicativeAmountSunset=0.0 
MultiplicativeAmountNight=0.0 
MultiplicativeAmountInteriorDay=0.0 
MultiplicativeAmountInteriorNight=0.0 
//滤光因素 
//该项比AdditiveAmount更加精细，它使用天气环境RGB颜色添加这种效果，数值越大，颜色添加量越大。 

ReflectiveAmountSunrise=0.1 
ReflectiveAmountDay=0.1 
ReflectiveAmountSunset=0.1 
ReflectiveAmountNight=0.1 
ReflectiveAmountInteriorDay=0.1 
ReflectiveAmountInteriorNight=0.1 
//反射量 
//IBL中的这个反射，是来自天空的反射，相比实时反射，不会考虑阴影遮蔽，颜色也是单一的。数值越高，物体顶部的反射越强。 



[RAIN] 
//雨 

IgnoreWeatherSystem=false 
//忽略天气系统 
//开启使自定义天气下对应该卷标内的ini选项被忽略，即enbseries.ini内这些选项设定会变成所有天气的全局设定。 

Enable=true 
//开启特效 
//开启雨滴特效。 

EnableAntialiasing=false 
//开启抗锯齿 
//开启使得雨滴更加平滑，在下雨时对性能有一定影响。 

EnableSupersampling=true 
//开启超级取样 
//开启该项使雨滴获得四倍的超级采样，在下雨时降帧较多。 

MotionStretch=1.0 
//位移拉伸 
//值越大雨点变成雨条状，雨滴原本的形状由ENBraindrops.tga贴图决定。 

MotionTransparency=0.9 
//位移透明 
//数值越大,雨越透明。 



[SUBSURFACESCATTERING] 
//皮下光线散射 

IgnoreWeatherSystem=false 
//忽略天气系统 
//开启使自定义天气下对应该卷标内的ini选项被忽略，即enbseries.ini内这些选项设定会变成所有天气的全局设定。 

EnableTextureAlpha=true 
//开启ALPHA贴图检测 
//开启该项使得皮下光线被贴图的ALPHA图层所覆盖，会使散射效果更加真实。 

Quality=0 
//品质 
//皮下反射降帧非常少的，几乎可以忽略不计。 

Radius=5.0 
//皮下光线散射范围 
//光线在皮下衍射的面积越大，人物就越像一块半透明的玉，人物本身的五官立体感会被模糊掉很多，所以这个数值不宜过高，也不宜过低。 

Amount=1.0 
//皮下总体光线强度 
//皮下光线散射的效果百分比，0相当于被关闭，1相当于全部开启，0.5就相当于总体的一半的效果。 

EpidermalAmount=4.0 
//表皮层光线散射强度 
//调节光线在表皮层反射的强度。 

SubdermalAmount=12.0 
//真皮层光线散射强度 
//调节光线在真皮层反射的强度。 

EpidermalDiffuseSaturation=0.0 
//表皮层血色浓度 
//控制人物表皮层的皮肤血色的浓淡，调节成1人会变成刚从桑拿出来的样子，调节成-1皮下反射将没有任何血色。 

SubdermalDiffuseSaturation=0.9 
//真皮层血色浓度 
//控制人物真皮层的皮肤血色的浓淡，调节成1人会变成刚从桑拿出来的样子，调节成-1皮下反射将没有任何血色。 

EpidermalMix=0.5 
SubdermalMix=0.5 
//表皮/真皮层混合 
//表皮/真皮层所占的比例。 

SubdermalTranslucency=0.3 
//真皮层透明度 
//数值越高，真皮部分越透明。 

SubdermalPhase=0.0 
//真皮层渐隐 
//数值越高，真皮部分越渐隐。 



[WATER] 
//水面特效 

IgnoreWeatherSystem=false 
//忽略天气系统 
//开启使自定义天气下对应该卷标内的ini选项被忽略，即enbseries.ini内这些选项设定会变成所有天气的全局设定。 

EnablePreCache=false 
//开启预缓存 
//为水面特效设置缓存区域。 

EnableDispersion=true 
//开启离散 
//添加在光照下会出现彩虹条状的效果。 

EnableCaustics=true 
//开启焦散 
//添加在浅水区域的波纹投影的效果。 

EnableParallax=false 
//开启视差 
//视差类水质材必须开启。 

EnableShadow=false 
//开启阴影 
//在水底模拟出受物体遮挡光线的阴影。 

EnableVolumetricShadow=false 
//开启体积阴影 
//在水面上模拟出受物体遮挡光线的阴影。 

EnableLighting=true 
//开启光照 
//利用光照影响水面的色彩效果。 

EnableSelfReflection=true 
//开启自反射 
//添加水面自反射的效果（非实时反射，仅反射山体或者云层等静态远景）。 

EnableDisplacement=false 
//开启变位 
//开启此项，视差类水质材才有移位元的效果，对性能的影响较大。 

DisplacementQuality=1 
//变位品质 
//质量越高，水面的视差效果越好。 

DisplacementFilterQuality=2 
//变位过滤品质 
//质量越高，视差产生的锯齿越少。 

ShadowQuality=0 
//阴影质量 
//质量越高，水面上的阴影效果越细致。 

EnableShadowNoise=false 
//开启阴影噪点 
//添加水面阴影边缘的噪点。 

ReflectionAmount=1.0 
//反射强度 
//周围景物（实际上只有远景LOD）反射的百分比。 

FrennelMultiplier=1.0 
//水体漫反射系数 
//数值越高，反射的亮度越高。 

FrennelMin=0.0 
FrennelMax=1.0 
//水体漫反射阈值 
//定义了水体漫反射亮度的最高与最低量。 

DispersionAmount=1.0 
//离散量 
//数值越高，散射产生的彩虹条纹量越多。 

CausticsAmount=3.0 
//焦散量 
//数值越高，浅水处的波纹投影量越多。 
 
SunSpecularMultiplier=1.0 
//阳光窥镜系数 
//数值越高，阳光在水面上的反射越亮。 
 
SunScatteringMultiplier=1.0 
//阳光散射系数 
//数值越高，阳光通过水体产生的散射数量越多。 
 
SunLightingMultiplier=0.3 
//阳光照明系数 
//数值越高，水体受阳光照射产生的亮度越高。 
 
WetMultiplier=1.0 
//潮湿系数 
//浅水处的水体透明度。 
 
WavesAmplitudeSunrise=1.0 
WavesAmplitudeDay=1.0 
WavesAmplitudeSunset=1.0 
WavesAmplitudeNight=1.0 
WavesAmplitudeInteriorDay=1.0 
WavesAmplitudeInteriorNight=1.0 
//波浪振幅 
//波浪的三维效果，不建议设置太高，否则难以观察到阳光散射。 

Muddiness=0.5 
//水体混浊度 
//数值越高，水体混浊度越高，过高会使得夜晚水体发亮。 



[UNDERWATER] 
//水下特效 

IgnoreWeatherSystem=false 
//忽略天气系统 
//开启使自定义天气下对应该卷标内的ini选项被忽略，即enbseries.ini内这些选项设定会变成所有天气的全局设定。 

EnableDispersion=true 
//开启离散 
//添加在光照下会出现彩虹条状的效果。 

EnableParallax=true 
//开启视差 
//视差类水质材必须开启。 

EnableDisplacement=true 
//开启变位 
//添加水下观看水面时的移位效果。 

EnableSilhouette=true 
//开启轮廓 
//Boris Vorontsov的解释：当变位（Displacement）开启时才有作用，该项会使得水面看起来更加立体，其中z缓冲资料（z-buffer data）改动是根据变位高度（displacement height）来决定的。 

EnableBlurring=false 
//开启模糊 
//添加水下的朦胧效果。 

EnableReflection=true 
//开启反射 
//添加水下的水面反射。 

EnableShadow=true 
//开启阴影 
//添加在水下光线受物体遮挡产生的阴影。 

ShadowQuality=0 
//阴影质量 
//质量越高，水下的阴影越细致。 

DispersionAmount=1.0 
//离散量 
//数值越高，水下的人物轮廓以及水面受光照产生的彩虹条纹越明显。 

ReflectionAmount=1.0 
//反射量 
//数值越高，水下的水面的反射越通透。 

TransparencyFade=1.0 
//透明度淡出 
//定义水下透明度的淡出距离，数值越大，模糊将会由远及近。 

TransparencyCurve=1.0 
//透明度曲线 
//数值越大，不透明处的曲线值变高，变得更加透明。 

DeepnessFade=2.0 
//深度淡出 
//定义水下深度的淡出距离，数值越大，黑暗将会由远及近。 

DeepnessDarkening=0.5 
//深度暗化 
//数值越大，定义为黑暗处的地方将越加黑暗。 

TintAmount=2.0 
//色彩量 
//数值越高，水下的模糊的色彩饱和度越高。 

TintFade=2.0 
//色彩淡出 
//数值越高，彩色将由远及近靠近镜头。 

SunScatteringMultiplier=0.5 
//阳光散射系数 
//数值越高，阳光在水下的散射数量越多。 



[CLOUDSHADOWS] 
//云层投影 
 
IgnoreWeatherSystem=false 
//忽略天气系统 
//开启使自定义天气下对应该卷标内的ini选项被忽略，即enbseries.ini内这些选项设定会变成所有天气的全局设定。 
 
EnableAtNight=false 
//夜间有效 
//开启该项使得夜晚也有云层投影的效果。 
 
OpacitySunrise=1.0 
OpacityDay=1.0 
OpacitySunset=1.0 
OpacityNight=1.0 
OpacityInteriorDay=1.0 
OpacityInteriorNight=1.0 
//云层透光性 
//此项数值越高，云层下环境与人物的光照度对比度被减弱，模拟被云层阻挡大量光线的效果。 



[VOLUMETRICRAYS] 
//体积射线 

IgnoreWeatherSystem=false 
//忽略天气系统 
//开启使自定义天气下对应该卷标内的ini选项被忽略，即enbseries.ini内这些选项设定会变成所有天气的全局设定。 

Quality=1 
//品质 
//质量越高，射线的效果越好。 

IntensitySunrise=0.2 
IntensityDay=0.2 
IntensitySunset=0.2 
IntensityNight=0.2 
IntensityInteriorDay=0.2 
IntensityInteriorNight=0.2 
//射线亮度 
//数值越高，太阳经云层遮挡后的射线亮度越高，同时一定程度提高整体天空亮度。 

DensitySunrise=5.0 
DensityDay=1.0 
DensitySunset=1.0 
DensityNight=1.0 
DensityInteriorDay=1.0 
DensityInteriorNight=1.0 
//射线密度 
//数值越高，太阳经云层遮挡后的射线越密集。 

SkyColorAmountSunrise=1.0 
SkyColorAmountDay=1.0 
SkyColorAmountSunset=1.0 
SkyColorAmountNight=0.5 
SkyColorAmountInteriorDay=0.5 
SkyColorAmountInteriorNight=0.5 
//天空色泽 
//数值越高，天空受到的射线色彩的影响越明显。 



[PROCEDURALSUN] 
//太阳处理 

IgnoreWeatherSystem=false 
//忽略天气系统 
//开启使自定义天气下对应该卷标内的ini选项被忽略，即enbseries.ini内这些选项设定会变成所有天气的全局设定。 

Size=1.0 
//太阳大小 
//数值越高，太阳越大，同时会影响许多和太阳有关的参数效果。 

EdgeSoftness=0.4 
//太阳边缘柔度 
//数值越高，太阳的边缘越柔和。 

GlowIntensitySunrise=0.4 
GlowIntensityDay=0.4 
GlowIntensitySunset=0.4 
GlowIntensityNight=0.4 
GlowIntensityInteriorDay=0.4 
GlowIntensityInteriorNight=0.4 
//辉光强度 
//数值越高，太阳经物体遮挡后的光线越亮度越高。 

GlowCurveSunrise=10.0 
GlowCurveDay=10.0 
GlowCurveSunset=10.0 
GlowCurveNight=10.0 
GlowCurveInteriorDay=10.0 
GlowCurveInteriorNight=10.0 
//辉光曲线 
//数值越高，太阳经物体遮挡后的光线衰减程度越低，辉光越长。 



[MIST] 
//雾气特效 

IgnoreWeatherSystem=false 
//忽略天气系统 
//开启使自定义天气下对应该卷标内的ini选项被忽略，即enbseries.ini内这些选项设定会变成所有天气的全局设定。 

EnableAnchors=true 
//开启自定义雾区 
//可以在勾选show objects window后，在右边的Anchor窗口自定义雾区。雾区文件将会以mist_anchors.xml的名称保存在enbseries文件夹里。 

AnchorsAmountSunrise=1.0 
AnchorsAmountDay=1.0 
AnchorsAmountSunset=1.0 
AnchorsAmountNight=1.0 
AnchorsAmountInteriorDay=1.0 
AnchorsAmountInteriorNight=1.0 
//雾区雾气数量 
//控制在设定自定义雾区的雾气的数量。 

ColorFromEnvironmentFog=0.0 
//环境雾色彩 
//数值越大迭加山体色调的量越多。 

SkyLightingAmountSunrise=1.0 
SkyLightingAmountDay=1.0 
SkyLightingAmountSunset=1.0 
SkyLightingAmountNight=1.0 
SkyLightingAmountInteriorDay=1.0 
SkyLightingAmountInteriorNight=1.0 
//天空照明量 
//数值越高，远处山体雾气越偏亮。 

SunLightingAmountSunrise=0.1 
SunLightingAmountDay=0.1 
SunLightingAmountSunset=0.1 
SunLightingAmountNight=0.1 
SunLightingAmountInteriorDay=0.1 
SunLightingAmountInteriorNight=0.1 
//日光照明量 
//数值越高，雾气受太阳光照的影响越强烈。 

DesaturationSunrise=0.0 
DesaturationDay=0.0 
DesaturationSunset=0.0 
DesaturationNight=0.0 
DesaturationInteriorDay=0.0 
DesaturationInteriorNight=0.0 
//雾气去饱和度 
//数值越高，雾气色调越被抵消，颜色越淡。 

ColorFilterSunrise=1, 1, 1 
ColorFilterDay=1, 1, 1 
ColorFilterSunset=1, 1, 1 
ColorFilterNight=1, 1, 1 
ColorFilterInteriorDay=1, 1, 1 
ColorFilterInteriorNight=1, 1, 1 
//雾气滤镜RGB通道 
//可设置雾的颜色。 

RelativeToCameraSunrise=0.0 
RelativeToCameraDay=0.0 
RelativeToCameraSunset=0.0 
RelativeToCameraNight=0.0 
RelativeToCameraInteriorDay=0.0 
RelativeToCameraInteriorNight=0.0 
//相对镜头 
//数值越大，雾气浓度不变，但雾气越向镜头方向靠拢。 

VerticalOffsetSunrise=-10.0 
VerticalOffsetDay=-10.0 
VerticalOffsetSunset=-10.0 
VerticalOffsetNight=-10.0 
VerticalOffsetInteriorDay=-10.0 
VerticalOffsetInteriorNight=-10.0 
//垂直偏移 
//控制雾气的垂直偏移高度，该高度决定了雾气的初始位置。 

DensitySunrise=1.5 
DensityDay=1.5 
DensitySunset=1.5 
DensityNight=1.5 
DensityInteriorDay=1.5 
DensityInteriorNight=1.5 
//雾气密度 
//数值越大，雾气浓度越大，若雾气浓度足够大到进入镜头周围，则数值越大，镜头周围清晰范围越小。 

VerticalFadeSunrise=4.0 
VerticalFadeDay=4.0 
VerticalFadeSunset=4.0 
VerticalFadeNight=4.0 
VerticalFadeInteriorDay=4.0 
VerticalFadeInteriorNight=4.0 
//垂直淡出 
//数值越大，垂直偏移位置上的线性淡出越弱。 

DistanceFadeSunrise=1.0 
DistanceFadeDay=1.0 
DistanceFadeSunset=1.0 
DistanceFadeNight=1.0 
DistanceFadeInteriorDay=1.0 
DistanceFadeInteriorNight=1.0 
//雾气淡出距离 
//数值越大，雾气在水平方向上淡出越明显。 
 
BottomTopSunrise=0.0 
BottomTopDay=0.0 
BottomTopSunset=0.0 
BottomTopNight=0.0 
BottomTopInteriorDay=0.0 
BottomTopInteriorNight=0.0 
//雾气遮盖方向 
//数值0表示雾气由垂直偏移高度（VerticalOffset）向下遮盖，数值1则表示向上遮盖。 
 
ExponentialFadeSunrise=1.0 
ExponentialFadeDay=1.0 
ExponentialFadeSunset=1.0 
ExponentialFadeNight=1.0 
ExponentialFadeInteriorDay=1.0 
ExponentialFadeInteriorNight=1.0 
//指数型淡出 
//可视之为雾总体效果的可视范围。一个较大的数值可令以上各项数值近乎无效，此数值越小雾效果越明显。 
